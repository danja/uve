{
  "source": {
    "include": ["src", "README.md"],
    "exclude": ["node_modules"]
  },
  "opts": {
    "destination": "./docs/generated",
    "recurse": true,
    "template": "node_modules/docdash"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false
  },
  "docdash": {
    "static": true,
    "sort": true
  }
}
